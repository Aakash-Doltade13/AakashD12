<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<bean id="car1" class="car.driver.Car">
		<property name="regnum" value="102030"></property>
		<property name="bname" value="ScorpioN"></property>
		<property name="colour" value="White"></property>
	</bean>
	
	<bean id="car2" class="car.driver.Car">
		<property name="regnum" value="102031"></property>
		<property name="bname" value="ScorpioN-1"></property>
		<property name="colour" value="Black"></property>
	</bean>
	
	
	
</beans>